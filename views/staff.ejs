<!-- This is the part of staff  -->

<section id="container" class="sub-page">
	<div class="wrap-container zerogrid">
		<div class="crumbs">
			<ul>
				<li><a href="index.html">Home</a></li>
				<li><a href="staff.html">Staff</a></li>
			</ul>
		</div>
		<div id="main-content">
			<div class="wrap-content">
				<div class="chef">
					<div class="row">
						<!-- ejs operation here -->
						<% staffData.forEach((elem)=>{ %>
							<div class="col-1-4">
								<div class="wrap-col">
									<div class="zoom-container">
										<a href="#">
											<img src="<%= elem.url %>" />
										</a>
									</div>
									<h3 class="bg-indigo-300  rounded-full animate-bounce">
										<%= elem.name %>
									</h3>
									<ul class="social t-center">
										<li><a style="color: rgb(59, 143, 143);" href="#"><i
													class="fa fa-twitter"></i></a></li>
										<li><a style="color: rgb(59, 143, 143);" href="#"><i
													class="fa fa-facebook"></i></a></li>
										<li><a style="color: rgb(59, 143, 143);" href="#"><i
													class="fa fa-linkedin"></i></a></li>
										<li><a style="color: rgb(59, 143, 143);" href="#"><i
													class="fa fa-instagram"></i></a></li>
									</ul>
								</div>
							</div>
							<% }) %>
								<!-- ejs operation end here -->






					</div>
				</div>
			</div>
		</div>
	</div>
</section>